<?xml version="1.0"?>

<portlet-app xmlns="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd" version="2.0">
	<portlet>
		<portlet-name>districts</portlet-name>
		<display-name>districts</display-name>
		<portlet-class>districts.RegisterOfDistrictsWebPortlet</portlet-class>
		<init-param>
			<name>view-jsp</name>
			<value>/jsp/districts/view.jsp</value>
		</init-param>
		<supports>
			<mime-type>text/html</mime-type>
		</supports>
		<resource-bundle>/content/Language_en</resource-bundle>
		<portlet-info>
			<title>Register of Districts</title>
			<short-title>Register of Districts</short-title>
			<keywords>Register of Districts</keywords>
		</portlet-info>
		<security-role-ref>
			<role-name>administrator</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>guest</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>power-user</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>user</role-name>
		</security-role-ref>
	</portlet>
	<portlet>
		<portlet-name>fermers</portlet-name>
		<display-name>fermers</display-name>
		<portlet-class>farmers.RegisterOfFarmersWebPortlet</portlet-class>
		<init-param>
			<name>view-jsp</name>
			<value>/jsp/farmers/view.jsp</value>
		</init-param>
		<supports>
			<mime-type>text/html</mime-type>
		</supports>
		<resource-bundle>/content/Language_en</resource-bundle>
		<portlet-info>
			<title>Register of Farmers</title>
			<short-title>Register of Farmers</short-title>
			<keywords>Register of Farmers</keywords>
		</portlet-info>
		<security-role-ref>
			<role-name>administrator</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>guest</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>power-user</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>user</role-name>
		</security-role-ref>
	</portlet>
</portlet-app>